		function PageItens:Toggle(text, callback)
		local Toggle = Instance.new("Frame")
        local UICorner = Instance.new("UICorner")
        local TextLabel = Instance.new("TextLabel")
        local TextButton = Instance.new("TextButton")
        local Fund = Instance.new("ImageLabel")
        local Circle = Instance.new("ImageLabel")
	    local debounce = false
        
        text = text or "New Toggle"
        callback = callback or function() end 
        
        
        Toggle.Name = "Toggle"
        Toggle.Parent = Page
        Toggle.BackgroundColor3 = Color3.fromRGB(45, 43, 41)
        Toggle.Size = UDim2.new(0, 404, 0, 43)
        
        UICorner.Parent = Toggle
        
        TextLabel.Parent = Toggle
        TextLabel.BackgroundColor3 = Color3.fromRGB(34, 32, 31)
        TextLabel.BackgroundTransparency = 1.000
        TextLabel.Position = UDim2.new(0.0445544571, 0, -0.0224921647, 0)
        TextLabel.Size = UDim2.new(0, 221, 0, 43)
        TextLabel.Font = Enum.Font.SourceSans
        TextLabel.Text = text
        TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        TextLabel.TextSize = 30.000
        TextLabel.TextWrapped = true
        TextLabel.TextXAlignment = Enum.TextXAlignment.Left
        
        TextButton.Parent = Toggle
        TextButton.BackgroundColor3 = Color3.fromRGB(49, 49, 49)
        TextButton.BackgroundTransparency = 1.000
        TextButton.BorderSizePixel = 0
        TextButton.Position = UDim2.new(0.792165637, 0, 0.186046511, 0)
        TextButton.Size = UDim2.new(0, 48, 0, 26)
        TextButton.Font = Enum.Font.SourceSans
        TextButton.Text = ""
        TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
        TextButton.TextSize = 14.000
        
        Fund.Name = "Fund"
        Fund.Parent = TextButton
        Fund.Active = true
        Fund.AnchorPoint = Vector2.new(0.5, 0.5)
        Fund.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
        Fund.BackgroundTransparency = 1.000
        Fund.Position = UDim2.new(0.510192871, 0, 0.5, 0)
        Fund.Selectable = true
        Fund.Size = UDim2.new(1.02038574, 0, 1, 0)
        Fund.Image = "rbxassetid://3570695787"
        Fund.ImageColor3 = Color3.fromRGB(35, 35, 35)
        Fund.ScaleType = Enum.ScaleType.Slice
        Fund.SliceCenter = Rect.new(100, 100, 100, 100)
        Fund.SliceScale = 0.667
        
        Circle.Name = "Circle"
        Circle.Parent = TextButton
        Circle.Active = true
        Circle.AnchorPoint = Vector2.new(0.5, 0.5)
        Circle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        Circle.BackgroundTransparency = 1.000
        Circle.Position = UDim2.new(0.251, 0, 0.440, 0)
        Circle.Selectable = true
        Circle.Size = UDim2.new(0, 22, 0, 23)
        Circle.Image = "rbxassetid://3570695787"
        Circle.ScaleType = Enum.ScaleType.Slice
        Circle.SliceCenter = Rect.new(100, 100, 100, 100)
        Circle.SliceScale = 0.667
	
	local toggled = false
	TextButton.MouseButton1Click:Connect(function()
	    toggled = not toggled
	    callback(toggled)
	    if toggled then
	    game:GetService("TweenService"):Create(Fund, TweenInfo.new(0.25), {ImageColor3 = Color3.fromRGB(0, 222, 130)}):Play()
	    game:GetService("TweenService"):Create(Circle, TweenInfo.new(0.25), {Position = UDim2.new(0.772, 0,0.440, 0)}):Play()
	    else
	    game:GetService("TweenService"):Create(Fund, TweenInfo.new(0.25), {ImageColor3 = Color3.fromRGB(35, 35, 35)}):Play()
		game:GetService("TweenService"):Create(Circle, TweenInfo.new(0.25), {Position = UDim2.new(0.251, 0,0.440, 0)}):Play()
	    end
	end)
end
		
		function PageItens:TextBox(info, placeholder, callback)
		info = info or "Info"
        placeholder = placeholder or "Type Here"
        callback = callback or function() end    
		    
        local TextB = Instance.new("Frame")
        local UICorner = Instance.new("UICorner")
        local TextBox = Instance.new("TextLabel")
        local TextBox_2 = Instance.new("TextBox")
        local UICorner_2 = Instance.new("UICorner")

        TextB.Name = "TextB"
