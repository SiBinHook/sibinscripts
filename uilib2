    local Tabs = {}
	function Tabs:NewTab(tabname)
	    tabname = tabname or "New Tab"
	    local PageButton = Instance.new("TextButton")
	    local Page = Instance.new("ScrollingFrame")
        local UIListLayout = Instance.new("UIListLayout")
        
        allFrames.Name = "allFrames"
        allFrames.Parent = MainFrame
        allFrames.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        allFrames.Position = UDim2.new(0.298000008, 0, 0.107000001, 0)
        allFrames.Size = UDim2.new(0, 394, 0, 335)
        allFrames.ZIndex = 0
        
        Page.Name = "Page"
        Page.Parent = allFrames
        Page.Active = true
        Page.BackgroundColor3 = Color3.fromRGB(34, 33, 32)
        Page.BorderSizePixel = 0
        Page.Size = UDim2.new(0, 394, 0, 335)
        Page.ScrollBarThickness = 0
        
        UIListLayout.Parent = Page
        UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
        UIListLayout.Padding = UDim.new(0, 6)
	    
	    PageButton.Name = "PageButton"
	    PageButton.Parent = TabHolder
	    PageButton.Text = tabname
	    PageButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	    PageButton.BackgroundTransparency = 1.000
	    PageButton.Size = UDim2.new(0, 146, 0, 50)
	    PageButton.Font = Enum.Font.SourceSans
	    PageButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    	PageButton.TextSize = 30.000
	    PageButton.TextWrapped = true
        
        
        PageButton.MouseButton1Click:Connect(function()
            for _, v in pairs(allFrames:GetChildren()) do
                if v:IsA("Frame") or v:IsA("ScrollingFrame") then
                    v.Visible = false
                end
            end
            Page.Visible = true
        end)
	    UICorner_2.Parent = PageButton

   local PageItens = {}
        function PageItens:Label(text)
            local Label = Instance.new("Frame")
