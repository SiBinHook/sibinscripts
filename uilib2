    function createMbutton()
        local MButton = Instance.new("Frame")
        local ImageButton = Instance.new("ImageButton")
        local UICorner = Instance.new("UICorner")

        MButton.Name = "MButton"
        MButton.Parent = SiBinUi
        MButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        MButton.BackgroundTransparency = 1.000
        MButton.Position = UDim2.new(0.442, 0,-0.014, 0)
        MButton.Size = UDim2.new(0, 96, 0, 94)
        
        ImageButton.Parent = MButton
        ImageButton.BackgroundColor3 = Color3.fromRGB(44, 44, 44)
        ImageButton.Position = UDim2.new(0.27, 0,0.271, 0)
        ImageButton.Size = UDim2.new(0, 45, 0, 45)
        ImageButton.Image = "http://www.roblox.com/asset/?id=10025278904"
        
        UICorner.Parent = ImageButton
        
        ImageButton.MouseButton1Click:Connect(function()
        SoundBalls:Play()
        MainFrame.Visible = true
        MainFrame:TweenPosition(UDim2.new(0, 386, 0, 226),"InOut","Sine",.2)
		task.wait(.2)
        task.wait(.1)
        MButton:Destroy()
        end)
    end
    
    game:GetService("UserInputService").InputBegan:Connect(function(Input, gameprocess)
        if not gameprocess then
            if Input.KeyCode == Enum.KeyCode.RightControl then
                if lolopen then
                    lolopen = false
                            SoundBalls:Play()
                            MainFrame.Visible = true
                            MainFrame:TweenPosition(UDim2.new(0, 386, 0, 226),"InOut","Sine",.2)
                    else
                    lolopen = true 
                            SoundBalls:Play()
                            MainFrame:TweenPosition(UDim2.new(0, 386, 0, -1000),"InOut","Sine",.2)
                            task.wait(0.2)
                            MainFrame.Visible = false
                end 
            end       
        end
    end)
    
